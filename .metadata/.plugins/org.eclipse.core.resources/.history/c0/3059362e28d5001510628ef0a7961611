import static org.junit.Assert.*;

import org.junit.Test;


public class FinderTest {
	
	Finder finder;
	String[][] puzzle;
	

	@Test
	public void test() {
		
		puzzle = new String[3][3];
		
		puzzle[0][0] = "a";
		puzzle[0][1] = "b";
		puzzle[0][2] = "c";
		puzzle[1][0] = "d";
		puzzle[1][1] = "e";
		puzzle[1][2] = "f";
		puzzle[2][0]= "g";
		puzzle[2][1] = "h";
		puzzle[2][2]= "i";
		System.out.println(puzzle);
		
		assertTrue(puzzle[0][0].equals("a"));
		
	}

}
